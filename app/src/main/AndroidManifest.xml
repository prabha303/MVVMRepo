<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.match.matchmakers">

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.launcher.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.launcher.view.LoginActivty"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.launcher.view.BrokerListActivty"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.launcher.view.RegisterActivty"
            android:screenOrientation="portrait" />




    </application>



</manifest>